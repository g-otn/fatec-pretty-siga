<nav>
    <div id="resumoAluno">
        <h4><%= perfil.name.replace(/(\B)[^ ]*/g, c => c.toLowerCase()) %>
        </h4>
        <div id="resumoAlunoImgAlunoContainer">
            <img src="<%= perfil.picture %>">
        </div>
        <table>
            <tbody>
                <tr>
                    <td>PP <%= Math.round(perfil.progress) %>%</td>
                    <td>PR <%= perfil.averageGrade %></td>
                </tr>
                <tr>
                    <td colspan="2">RA: <%= perfil.code %></td>
                </tr>
            </tbody>
        </table>
    </div>
    <ul id="menu">
        <li class="menuItem" onclick="mostrarDiv('divPerfil')">Perfil</li>
        <li class="menuItem" onclick="mostrarDiv('divNotasParciais')">Notas Parciais</li>
        <li class="menuItem" onclick="mostrarDiv('divDisciplinas')">Disciplinas</li>
        <li class="menuItem" onclick="mostrarDiv('divHorarios')">Horários</li>
        <li class="menuItem" onclick="mostrarDiv('divHistorico')">Histórico</li>
        <li class="menuItem" onclick="mostrarDiv('divGradeEscolar')">Grade Escolar</li>
    </ul>
    <ul>
        <li onclick="sair()">Sair</li>
    </ul>
    <img id="menuIconeSeta" class="icon" src="media/icon_arrow_left.png">
    <img id="menuIconeFixar" class="icon" src="media/icon_pin.png">
</nav>