<h2><%= (notasParciais[0].discipline.periodId + 1) %>º Período</h2>
<% notasParciais.forEach((e, i) => { %>
    <div class="disciplina">
        <div class="cabecalhoDisciplina">
            <h4><%= e.discipline.name %></h4>
            <p><%= disciplinas[i].teacherName.replace(/(\B)[^ ]*/g, c => c.toLowerCase()) %></p>
        </div>
        <table class="frequencia">
            <tbody>
                <tr>
                    <td>Presenças</td>
                    <td><%= disciplinas[i].presences %></td>
                    <td>Faltas</td>
                    <td><%= disciplinas[i].absenses %></td>
                </tr>
            </tbody>
        </table>
        <table>
            <thead>
                <tr>
                    <td>Média</td>
                    <td><%= e.discipline.grade %></td>
                </tr>
            </thead>
            <tbody>
                <% e.evaluations.forEach(notas => { %>
                    <tr>
                        <td><%= notas.description %></td>
                        <td><%= Number(notas.grades[0].score).toFixed(1) %></td>
                    </tr>
                <% }) %>
            </tbody>
        </table>
    </div>
<% }) %>